{
  "name": "nadar",
  "version": "1.0.0",
  "description": "AI-powered visual assistance app for Morocco's blind community",
  "private": true,
  "scripts": {
    "dev": "npm-run-all -p dev:server dev:app",
    "dev:server": "cd server && npm run dev",
    "dev:app": "cd app && npm start",
    "dev:tunnel": "node dev-tunnel.js",
    "dev:tunnel:simple": "node dev-tunnel-simple.js",
    "dev:tunnel:ps": "powershell -ExecutionPolicy Bypass -File dev-tunnel.ps1",
    "deploy:fly": "node deploy-fly.js",
    "deploy:fly:secure": "node deploy-fly-secure.js",
    "deploy:vercel": "node deploy-vercel.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@vercel/node": "^5.3.12",
    "@vercel/kv": "^3.0.0"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
